<%-partial("header",{desc:''})%>
<div id="container">
<style type="text/css">
	.dsq-textarea-reply{margin-left:0px!important;}
	.dsq-avatar{display:none!important;}
	#dsq-global-toolbar,
	#disqus_thread h3,
	#dsq-sort-by,#dsq-subscribe,
	#dsq-sort-select,#dsq-footer{
		display:none!important;
	}
	.dsq-comment-body{
		padding:0px!important;
	}
	#disqus_thread a{
		color:black!important;
	}
	.dsq-comment-text{
		color:black;
	}
</style>
<div id="disqus_thread" style="width:600px;margin:0 auto"></div>
</div>
<script type="text/javascript">
	/* * * CONFIGURATION VARIABLES: EDIT BEFORE PASTING INTO YOUR WEBPAGE * * */
	var disqus_shortname = 'pxm'; // required: replace example with your forum shortname

	/* * * DON'T EDIT BELOW THIS LINE * * */
	(function() {
		var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
		dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
		(document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
	})();
</script>
<%-partial("footer")%>